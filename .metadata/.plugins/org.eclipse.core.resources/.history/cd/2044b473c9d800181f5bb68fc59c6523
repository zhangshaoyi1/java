public class AbstrictChapterDetailSpidet extends AbstractSpider implements IChapterDetailSpider{

	@Override
	public ChapterDetail getChapterDetail(String url) {
		try {
			String result = super.crawl(url);
		} catch (Exception e) {
			throw new RuntimeException(e);
		}
		return null;
	}

}
